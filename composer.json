{
    "name": "orchestra/dusk-updater",
    "description": "Updater for Laravel Dusk ChromeDriver binaries",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "bin": [
        "dusk-updater"
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\DuskUpdater\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "ext-zip": "*",
        "composer-runtime-api": "^2.2",
        "composer/semver": "^1.5 || ^3.0",
        "symfony/console": "^6.0",
        "symfony/polyfill-ctype": "^1.9",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "laravel/dusk": "^7.6",
        "laravel/pint": "^1.3",
        "phpstan/phpstan": "^1.9.17",
        "phpunit/phpunit": "^10.0.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "stable"
}
